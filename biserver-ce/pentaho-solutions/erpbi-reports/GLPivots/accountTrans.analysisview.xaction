<?xml version="1.0" encoding="UTF-8"?>
<action-sequence> 
  <title>Account Transactions</title>
  <version>1</version>
  <logging-level>ERROR</logging-level>
  <documentation> 
    <author>joe</author>  
    <help/>  
    <result-type/>  
    <description>Account Transactions</description>  
    <icon/>  
    <!-- need an icon --> 
  </documentation>

  <inputs> 
    <mode type="string"> 
      <default-value/>  
      <sources> 
        <request>mode</request> 
      </sources> 
    </mode>  
    <YEAR type="string"> 
      <sources> 
        <request>YEAR</request> 
      </sources>  
      <default-value><![CDATA[2010]]></default-value> 
    </YEAR>  
    <COMPANY type="string"> 
      <sources> 
        <request>COMPANY</request> 
      </sources>  
      <default-value><![CDATA[All Companies]]></default-value> 
    </COMPANY>  
    <ACCOUNT type="string"> 
      <sources> 
        <request>ACCOUNT</request> 
      </sources>  
      <default-value><![CDATA[Accounts Payable, 01-01-2000-01]]></default-value> 
    </ACCOUNT>  
    <PROFITCENTER type="string"> 
      <sources> 
        <request>PROFITCENTER</request> 
      </sources>  
      <default-value><![CDATA[All Profit Centers]]></default-value> 
    </PROFITCENTER> 
  </inputs>

  <outputs> 
    <model type="string"/>  
    <connection type="string"/>  
    <mdx type="string"/>  
    <options type="list"/>  
    <title type="string"/>  
    <url type="string"> 
      <destinations> 
        <response>redirect</response> 
      </destinations> 
    </url>  
    <chartaxistickfontsize type="string"/>  
    <chartslicerfontsize type="string"/>  
    <chartlegendfontfamily type="string"/>  
