        <charttitlefontfamily><![CDATA[SansSerif]]></charttitlefontfamily>  
        <query><![CDATA[SELECT 
  NON EMPTY { 
	[Measures].[Date, Order],
	[Measures].[Days, Order to Delivery],
	[Measures].[Balance, Orders Unfulfilled],
	[Measures].[Interest, O2D Impact]
  } ON COLUMNS,

  NON EMPTY {
        Crossjoin(
          {[Fiscal Period.Fiscal Period CL].[{YEAR}]}
          , {[Order].[All Orders]}
       	 )
  } ON ROWS

FROM [SOByPeriod]
WHERE
(
	[Customer.Customer Code].[{CUSTOMER}],
	[Product.Product by Class].[{PRODUCTCLASS}],
      	[Product.Product Code].[{PRODUCTCODE}],
      	[Product.Product by Category].[{PRODUCTCATEGORY}],
      	[Account Rep.Account Reps by Code].[{ACCOUNTREP}],
      	[Ship City.Ship Region].[{SHIPREGION}]
)]]></query> 
